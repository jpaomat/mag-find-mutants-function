{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      false
    ],
    "no-console": [
      false,
      "log"
    ],
    "whitespace": [
      true,
      "check-module"
    ]
  },
  "rulesDirectory": []
}